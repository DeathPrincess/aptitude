#, c-format
msgid ""
" --with(out)-recommends\tSpecify whether or not to treat recommends as\n"
"                strong dependencies\n"
msgstr ""
" --with(out)-recommends\tNastaví, či sa majú odprúčané závislosti\n"
"                brať ako silné závislosti\n"

