msgstr ""

# type: Content of: <refentry><refsect1><itemizedlist><listitem><para>
#. type: Content of: <refentry><refsect1><itemizedlist><listitem><para>
#: en/manpage.xml:2370
msgid "<filename>/var/cache/apt/*.bin</filename>"
msgstr ""

# type: Content of: <refentry><refsect1><itemizedlist><listitem><para>
#. type: Content of: <refentry><refsect1><itemizedlist><listitem><para>
